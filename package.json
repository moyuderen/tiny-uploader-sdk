{
  "name": "tiny-uploader-sdk",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "大文件分片上传解决方案sdk, 可用于各种UI框架",
  "keywords": [
    "uploader",
    "upload",
    "file",
    "big file",
    "retry",
    "pause",
    "resume",
    "上传",
    "分片上传",
    "大文件",
    "并发",
    "重试",
    "断点续传",
    "秒传"
  ],
  "license": "MIT",
  "author": "moyuderen",
  "homepage": "https://github.com/moyuderen/tiny-uploader-sdk/quick-start.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/moyuderen/tiny-uploader-sdk"
  },
  "bugs": {
    "url": "https://github.com/moyuderen/tiny-uploader-sdk/issues"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "module": "./dist/uploader.es.js",
  "main": "./dist/uploader.umd.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/uploader.cjs.js",
      "import": "./dist/uploader.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "hashion": "^0.0.11"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  }
}