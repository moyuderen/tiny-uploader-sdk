{
  "name": "@tinyuploader/sdk",
  "version": "2.3.0-alpha.5",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "大文件分片上传解决方案sdk, 可用于各种UI框架",
  "keywords": [
    "uploader",
    "upload",
    "file",
    "big file",
    "retry",
    "pause",
    "resume",
    "上传",
    "分片上传",
    "大文件",
    "并发",
    "重试",
    "断点续传",
    "秒传"
  ],
  "license": "MIT",
  "author": "moyuderen",
  "homepage": "https://moyuderen.github.io/uploader/sdk/quick-start.html",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "module": "./dist/uploader.es.js",
  "main": "./dist/uploader.umd.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/uploader.cjs.js",
      "import": "./dist/uploader.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "change:alpha": "pnpm changeset pre enter alpha",
    "change:beta": "pnpm changeset pre enter beta",
    "change:rc": "pnpm changeset pre enter rc",
    "change:stable": "pnpm changeset pre exit",
    "change": "pnpm changeset",
    "change-version": "pnpm changeset version",
    "push-npm": "pnpm install && pnpm build && pnpm -F @tinyuploader/sdk publish"
  },
  "peerDependencies": {
    "hashion": "^0.0.11"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^24.0.1",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}